				<POSTCODE/>
				<ORGANISATION/>
				<LONGITUDE/>
				<LATITUDE/>
				<ADDRTYPE/>
				<EMAIL/>
				<TEL/>
				<FAX/>
				<COUNTRY/>
				<COUNTRY_CODE/>
				<ADDRESS_ID/>
				<INACTIVE>0</INACTIVE>
				<CONTACTS>
					<CONTACT>
						<TITLE/>
						<FORENAME/>
						<SURNAME/>
						<JOBTITLE/>
						<TEL/>
						<FAX/>
						<MOBILE/>
						<EMAIL/>
