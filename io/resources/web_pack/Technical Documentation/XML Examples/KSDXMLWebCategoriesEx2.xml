				<SELL_PRICE>0</SELL_PRICE>
				<CANONICAL_ITEM>0</CANONICAL_ITEM>
			</STOCK_ITEM>
		</CATEGORY>
		<STOCK_ITEM>
			<STOCK_ID>19651</STOCK_ID>
			<STOCK_CODE>KSDMISCITEM</STOCK_CODE>
			<CATEGORY_STOCK_CODE>Misc Code</CATEGORY_STOCK_CODE>
			<LONG_DESC>Misc Long Description</LONG_DESC>
			<SHORT_DESC>Misc Description</SHORT_DESC>
			<WEB_TEASER>Misc Web Teaser</WEB_TEASER>
			<SELL_PRICE>0</SELL_PRICE>
			<CANONICAL_ITEM>-1</CANONICAL_ITEM>
		</STOCK_ITEM>
	</CATEGORY>
